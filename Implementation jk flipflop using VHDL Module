begin

input <=j&k

process (clk, j, k, p,c)

variable temp:std_logic:'0'

begin

if (c=1 and p=1) then

if rising edge (clk) then

case input is

when '10' => temp:'1'
 when '01' =>temp:'0'

when '11' temp:not temp:

when others -> null;

end case:

end if;
else
temp=0

abar < not temp: 
end process

